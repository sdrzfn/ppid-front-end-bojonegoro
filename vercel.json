{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }

    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/profile",
            "dest": "/api/profil.php"
        },
        {
            "src": "/layanan",
            "dest": "/api/layanan.php"
        }
    ]
}